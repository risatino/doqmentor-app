<section>
<h2><%= link_to 'Music Contracts', '/music-docs',  class: "btn btn-info" %> <%= link_to 'New Music Contract', '/music-docs/new', class: "btn btn-info" %> <%= link_to 'All Clients', '/clients', class: "btn btn-info" %></h2>
<br>
<h2>Existing Music Contracts</h2>
<% @music_contracts.each do |music_contract| %>
  <h3>Contract Title: <%= music_contract.title %></h3>
  <h3>Job ID: <a href="/music-docs/<%= music_contract.id %>"><%= music_contract.title %></a></h3>
  <h3>Gig Due Date: <%= music_contract.due_date %></h3>
<% end %>
</section>
